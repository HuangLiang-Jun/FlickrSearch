target 'FlickrSearch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'SnapKit'
  pod 'Moya'
  pod 'SVProgressHUD'
  pod 'IQKeyboardManagerSwift'
  pod 'Kingfisher'
  pod 'RealmSwift'
end
